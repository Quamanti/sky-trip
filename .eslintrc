{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": [
    "jsx-a11y"
  ],
  "rules": {
    "object-curly-newline": "off",
    "arrow-parens": "off",
    "import/prefer-default-export": "off",
    "linebreak-style": "off",
    "react/prop-types": [0],
    "react/jsx-props-no-spreading": [0],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}